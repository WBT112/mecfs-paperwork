{
  "name": "mecfs-paperwork-app",
  "private": true,
  "license": "Apache-2.0",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/assert-no-root-formpacks.mjs && node scripts/sync-funding.mjs",
    "prebuild": "node scripts/assert-no-root-formpacks.mjs && node scripts/sync-funding.mjs && npm run brand:generate",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && vite build --mode production",
    "build:staging": "tsc -b && vite build --mode staging",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "typecheck": "tsc --noEmit",
    "formpack:new": "node scripts/new-formpack.mjs",
    "preformpack:validate": "node scripts/assert-no-root-formpacks.mjs",
    "formpack:validate": "node scripts/validate-formpacks.mjs",
    "quality-gates": "node scripts/run-quality-gates.mjs",
    "pretest:e2e": "node scripts/assert-no-root-formpacks.mjs && node scripts/sync-funding.mjs",
    "test:e2e": "node scripts/run-e2e-soft.mjs",
    "test:e2e:a11y": "npx playwright test e2e/a11y.spec.ts --project=chromium",
    "test:e2e:strict": "npx playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test": "vitest --run",
    "test:unit": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "brand:generate": "node --import tsx/esm scripts/generate-brand-assets.ts",
    "brand:check": "node --import tsx/esm scripts/generate-brand-assets.ts --check",
    "prepare": "husky",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.2",
    "@rjsf/core": "^6.2.5",
    "@rjsf/utils": "^6.1.2",
    "@rjsf/validator-ajv8": "^6.2.5",
    "@types/sanitize-html": "^2.16.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "buffer": "^6.0.3",
    "docx-templates": "^4.15.0",
    "events": "^3.3.0",
    "i18next": "^23.12.0",
    "idb": "^8.0.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-i18next": "^14.1.3",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.30.3",
    "sanitize-html": "^2.17.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@resvg/resvg-js": "^2.6.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-sonarjs": "^3.0.6",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.2.0",
    "husky": "^9.1.7",
    "jpeg-js": "^0.4.4",
    "jsdom": "^27.4.0",
    "jszip": "^3.10.1",
    "lint-staged": "^16.2.7",
    "png-to-ico": "^2.1.8",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "@isaacs/brace-expansion": "^5.0.1",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.5"
  }
}
