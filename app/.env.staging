# Staging environment configuration
# This file is loaded by Vite when building with --mode staging

# Show dev-only formpacks (visibility: "dev") in staging
VITE_SHOW_DEV_FORMPACKS=true

# Deployment environment marker for staging banner
VITE_DEPLOYMENT_ENV=staging

# Absolute public origin for social previews (Open Graph/Twitter)
VITE_PUBLIC_ORIGIN=https://staging.mecfs-paperwork.de
